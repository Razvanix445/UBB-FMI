<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Label?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Button?>
<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.lab7.controller.EditUtilizatorController">
<children>
    <GridPane hgap="5.0" layoutX="50.0" prefHeight="207.0" prefWidth="373.0" vgap="5.0"
              AnchorPane.bottomAnchor="140.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="49.0"
              AnchorPane.topAnchor="10.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="208.5" minWidth="10.0" prefWidth="120.5"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="237.5" minWidth="10.0" prefWidth="237.5"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints/>
            <RowConstraints/>
        </rowConstraints>
        <children>
            <Label prefHeight="47.0" prefWidth="187.0" text="Id User:"/>
            <Label prefHeight="47.0" prefWidth="187.0" text="Nume" GridPane.rowIndex="1"/>
            <Label prefHeight="47.0" prefWidth="187.0" text="Prenume" GridPane.rowIndex="2"/>


            <TextField fx:id="textFieldId" prefHeight="46.0" prefWidth="186.0" GridPane.columnIndex="1"/>
            <TextField fx:id="textFieldNume" prefHeight="48.0" prefWidth="186.0" GridPane.columnIndex="1"
                       GridPane.rowIndex="1"/>
            <TextField fx:id="textFieldPrenume" prefHeight="49.0" prefWidth="186.0" GridPane.columnIndex="1"
                       GridPane.rowIndex="2"/>
        </children>
        <opaqueInsets>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </opaqueInsets>
    </GridPane>
    <ButtonBar layoutX="250.0" layoutY="262.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="20.0">
        <buttons>
            <Button mnemonicParsing="false" onAction="#handleSave" text="Save">
                <padding>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                </padding>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel"/>
        </buttons>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
    </ButtonBar>
</children>
</AnchorPane>
